function isInputNameInvalid({ name, regex }) {
    if (!regex) {
        regex = /[^A-Za-z0-9\-\_!?#$%&\[\](){}*+',.\/@=\^~:;| ]+/;
    }

    if (!name || !name.length) {
        return false;
    }

    var reg = new RegExp(regex, 'gim');

    return reg.test(name);
}

function isInputTextInvalid({ name, regex }) {
    if (!regex) {
        regex = /[^A-Za-z0-9\-\_!?#$%&\[\](){}*+',.\/@=\^~:;|\n|\r ]+/;
    }

    if (!name || !name.length) {
        return false;
    }

    var reg = new RegExp(regex, 'gim');

    return reg.test(name);
}

